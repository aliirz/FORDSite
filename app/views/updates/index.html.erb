<h1>Listing updates</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Posted on</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @updates.each do |update| %>
  <tr>
    <td><%= update.title %></td>
    <td><%= update.description %></td>
    <td><%= update.posted_on %></td>
    <td><%= link_to 'Show', update %></td>
  </tr>
<% end %>
</table>

<br />


